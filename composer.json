{
    "name": "zdz/laravel-middleware-log",
    "description": "laravel middleware for log auto record",
    "keywords": [
        "laravel",
        "middleware",
        "log",
        "auto",
        "laravel-middleware-log"
    ],
    "require": {
        "php": "^7.2",
        "illuminate/container": "^6.0",
        "illuminate/contracts": "^6.0",
        "illuminate/support": "^6.0",
        "illuminate/http": "^6.0",
        "Illuminate/database": "^6.0",
        "ext-json": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "zdz",
            "email": "zdzserver@163.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "zdz\\LaravelMiddlewareLog\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "zdz\\LaravelMiddlewareLog\\LogServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
